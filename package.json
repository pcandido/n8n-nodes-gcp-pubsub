{
    "name": "n8n-nodes-gcp-pubsub",
    "version": "1.3.3",
    "description": "Google Cloud Pub/Sub integration node for n8n",
    "license": "MIT",
    "homepage": "https://github.com/pcandido/n8n-nodes-gcp-pubsub",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "google-cloud",
        "pubsub",
        "gcp",
        "integration"
    ],
    "author": {
        "name": "Paulo Cândido",
        "email": "pcandido.m@gmail.com"
    },
    "contributors": [
        {
            "name": "Paulo Cândido",
            "email": "pcandido.m@gmail.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pcandido/n8n-nodes-gcp-pubsub.git"
    },
    "bugs": {
        "url": "https://github.com/pcandido/n8n-nodes-gcp-pubsub/issues"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/GcpServiceAccountApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/GcpPubSubPublisher/GcpPubSubPublisher.node.js",
            "dist/nodes/GcpPubSubSubscriber/GcpPubSubSubscriber.node.js"
        ]
    },
    "dependencies": {
        "@google-cloud/pubsub": "^5.2.0"
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
